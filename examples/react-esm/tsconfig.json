{
    "compileOnSave": true,
    "compilerOptions": {
        "sourceMap": true,
        "strict": false,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": false,
        "module": "ESNext",
        "target": "ESNext",
        "jsx": "preserve",
        "moduleResolution": "node",
        "baseUrl": "./",
        "rootDir": "./src",
        "paths": {
            "@/*": [
                "src/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}